{
  "name": "fractal_perimeter_calculator",
  "version": "1.0.0",
  "description": "A calculator which models and uses fractal dimensions to determine the length or perimeter of an object.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "asar pack Myapp-darwin-x64/Myapp.app/Contents/Resources/app Myapp-darwin-x64/Myapp.app/Contents/Resources/app.asar",
    "build": "electron-packager . Myapp --platform=darwin  && cp Icon.icns Myapp-darwin-x64/Myapp.app/Contents/Resources/electron.icns "
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "fractal",
    "calculator",
    "dimension",
    "perimeter",
    "area"
  ],
  "author": "Wesley Henshall",
  "license": "",
  "devDependencies": {
    "build:less": "lessc client/less/style.less public/css/style.css",
    "electron": "^1.4.3",
    "electron-builder": "^7.14.1",
    "electron-installer-dmg": "^0.1.2",
    "electron-packager": "^8.1.0",
    "less": "^2.7.1"
  },
  "dependencies": {
    "electron-window-manager": "^1.0.1",
    "express": "^4.14.0"
  }
}
